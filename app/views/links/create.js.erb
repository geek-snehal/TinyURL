var output = document.getElementById('tiny_url');
<% if @visited_link.present? %>
  var code = "<%= @short_link %>/<%= @visited_link.token %>"
  output.innerHTML = code
<% else %>
  var error = document.getElementById('error_msg');
  <% if @new_link.errors.present? %>
    console.log("<%= @new_link.errors.full_messages[0] %>")
    var message = "<%= @new_link.errors.full_messages[0] %>"
    error.innerHTML = message
  <% else %>
    var code = "<%= @short_link %>/<%= @new_link.token %>"
    error.innerHTML = ''
    output.innerHTML = code
  <% end %>
<% end %>  
