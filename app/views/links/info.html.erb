<div class="container">
  <div class="p-5">
    <div class="card">
      <div class="card-body">
      <div class="card-title"><%= @link.original_url %></div>
        <table class="table">
          <thead>
            <tr>
              <th scope="col">IP Address</th>
              <th scope="col">User Count</th>
            </tr>
          </thead>
          <tbody>
            <% @users.each do |user| %>
              <tr>
                <td><%= user.ip_addr %></td>
                <td><%= user.user_clicks %></td>
              </tr>
            <% end %>  
          </tbody>
       </table>
      </div>
    </div>
  </div>    
</div>